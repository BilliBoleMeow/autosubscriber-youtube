{
  "name": "autosubscriber-youtube",
  "description": "Python Selenium-based automated subscribing functions for youtube sub4sub websites",
  "repository": "https://github.com/atalaydenknalbant/autosubscriber-youtube",
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-chromedriver"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-google-chrome"
    }
  ],
  "env": {
    "HEROKU": {
      "description":"DON'T CHANGE 'Tells Program to run using heroku' DON'T CHANGE",
      "value": "available",
      "required": false
    },
    "CHROMEDRIVER_PATH": {
      "description":"DON'T CHANGE 'Chromedriver path' DON'T CHANGE",
      "value": "/app/.chromedriver/bin/chromedriver",
      "required": false
    },
    "GOOGLE_CHROME_BIN": {
      "description":"DON'T CHANGE 'Google Chrome path' DON'T CHANGE",
      "value": "/app/.apt/usr/bin/google-chrome",
      "required": false
    },
    "youtube_email": {
      "value": "Enter Youtube Email",
      "required": false
    },
    "youtube_password": {
      "value": "Enter Youtube Password",
      "required": false
    },
    "youtube_channel_id": {
      "value": "Enter Youtube Channel ID",
      "required": false
    },
    "youtube_useragent": {
      "value": "Enter GO TO chrome://version/ > User Agent > Copy",
      "required": false
    },
    "subpals_com_password": {
      "value": "Enter Subpals password",
      "required": false
    },
    "sonuker_com_password": {
      "value": "Enter Sonuker password",
      "required": false
    },
    "ytpals_com_password": {
      "value": "Enter Ytpals password",
      "required": false
    },
    "subscribers_video_email": {
      "value": "Enter Subscribers.video email",
      "required": false
    },
    "submenow_com_email": {
      "value": "Enter Submenow email",
      "required": false
    },
    "ytmonster_com_username": {
      "value": "Enter Ytmonster Username",
      "required": false
    },
    "ytmonster_com_password": {
      "value": "Enter Ytmonster Password",
      "required": false
    },
    "ytbpals_com_email": {
      "value": "Enter Ytbpals Email",
      "required": false
    },
    "ytbpals_com_password": {
      "value": "Enter Ytbpals Password",
      "required": false
